// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
datasource db {
  provider = "sqlite"
  url      = file("sqlite.db)
}

model User {
  id String @id @default(cuid())
  nome String
  email String
  createdAt DateTime @default(now())
}